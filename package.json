{
  "name": "actionsreducer",
  "version": "0.0.2",
  "description": "helper for creating redux actions and reducers",
  "main": "dist/index",
  "browser": "browser/index",
  "typings": "dist/index",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:example": "webpack --config example/webpack.compile.js",
    "build:server": "tsc",
    "build:client": "npm run build:client:min && npm run build:client:full",
    "build:client:min": "NODE_ENV=production MINIMIZE=true webpack",
    "build:client:full": "NODE_ENV=production webpack",
    "clean": "npm run clean:client && npm run clean:server",
    "clean:client": "rimraf browser",
    "clean:server": "rimraf dist",
    "dist": "npm run clean && npm run build",
    "test": "karma start",
    "test:once": "karma start --single-run",
    "prepublish": "npm run test:once && npm run clean && npm run build",
    "example": "node ./example/server.js",
    "typings": "typings"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "markdown-loader": "^0.1.7",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "prismjs": "^1.5.1",
    "raw-loader": "^0.5.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-fail-plugin": "^1.0.4"
  }
}
